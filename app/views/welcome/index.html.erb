<% if current_user %>
    <%= link_to "Log Out", logout_path, method: :delete %>
<% else %>
    <%= button_to "Create New User", register_path, method: :get %>
    <%= link_to "Log In", login_path %>
<% end %>
<section class='existing-users'>
    <p>Existing Users: </p>
    <% @users.each do |user| %>
        <% if current_user && user == current_user %>
            <li><%= link_to user.email, user_path(user.id) %></li>
        <% else %>
            <li><%= link_to user.email, login_path(email: user.email) %></li>
        <% end %>
    <% end %>
</section>