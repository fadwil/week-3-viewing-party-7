<% if session[:user_id] %>
    <%= link_to "Log Out", logout_path %>
<% else %>
    <%= button_to "Create New User", register_path, method: :get %>
    <%= link_to "Log In", login_path %>
<% end %>
<section class='existing-users'>
    <p>Existing Users: </p>
    <% @users.each do |user| %>
        <li><%= user.email %></li>
    <% end %>
</section>